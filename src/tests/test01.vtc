varnishtest "Test rate limiting vmod"

server s1 {
       rxreq
       txresp
} -start

varnish v1 -vcl+backend {
	import ratelimit from "${vmod_topbuild}/src/.libs/libvmod_ratelimit.so";

	sub vcl_deliver {
		if (ratelimit.lastseen(client.ip, "", "test") < 3s) {
		    error 500 "buhu";
                }
		if (ratelimit.lastseen(client.ip, "", "test") < 3s) {
		    error 200 "ok";
		}
        }
} -start

client c1 {
	txreq -url "/"
	rxresp
	expect resp.http.status == 200
}

client c1 -run
